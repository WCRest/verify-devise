%h2= I18n.t('verify_register_title', scope: 'devise')
= enable_verify_form do
  = text_field_tag :country_code, '', :autocomplete => :off, :placeholder => I18n.t('devise.country'), :id => "verify-countries"
  = text_field_tag :cellphone, '', :autocomplete => :off, :placeholder => I18n.t('devise.cellphone'), :id => "verify-cellphone"
  %p= submit_tag I18n.t('enable_verify', scope: 'devise')
